* color p3markfill     none
* color p4markfill     none
* color p5markfill     none
* color p6markfill     none
* color p1mark         yellow
* color p2mark         red
* color p3mark         blue
* color p#markfill   none
* color p#           yellow
* color p#line       yellow
* color p#shade      yellow
* color p#markback   yellow
* color p#markbkfill yellow
* color p#area       yellow
* color plabel       white


yesno mat_label_box          no
yesno mat_label_as_textbox   no

shadestyle plotregion none


linestyle plotregion  foreground


areastyle matrix_label        none

linepattern p1lineplot  solid
linepattern p2lineplot  solid
linepattern p3lineplot  solid
linepattern p4lineplot  longdash_dot
linepattern p5lineplot  longdash
linepattern p6lineplot  dash_dot
linepattern p7lineplot  dot
linepattern p8lineplot  shortdash_dot
linepattern p9lineplot  tight_dot
linepattern p10lineplot dash_dot_dot
linepattern p11lineplot longdash_shortdash
linepattern p12lineplot dash_3dot
linepattern p13lineplot longdash_dot_dot
linepattern p14lineplot shortdash_dot_dot
linepattern p15lineplot longdash_3dot

linecolor p1lineplot dknavy


symbol p1     circle_hollow
symbol p2     diamond_hollow
symbol p3     triangle_hollow
symbol p4     square_hollow
symbol p5     x
symbol p6     +
symbol p7     diamond
symbol p8     square
symbol p9     triangle

symbolsize p1 small
